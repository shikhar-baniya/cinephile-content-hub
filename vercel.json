{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite-api-base-url",
    "VITE_TMDB_API_KEY": "@vite-tmdb-api-key",
    "VITE_TMDB_BASE_URL": "@vite-tmdb-base-url",
    "VITE_TMDB_IMAGE_BASE_URL": "@vite-tmdb-image-base-url"
  }
}